<h1>New venue</h1>

<% form_for(@venue) do |f| %>
  <%= f.error_messages %>


  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </p>
  <p>
    <%= f.label :address_hint %><br />
    <%= f.text_field :address_hint %>
  </p>
  <p>
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </p>
  <p>
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </p>
  <p>
    <%= f.label :zip %><br />
    <%= f.text_field :zip %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :ical_feed_url %><br />
    <%= f.text_field :ical_feed_url %>
  </p>
  <p>
    <%= f.label :twitter_username %><br />
    <%= f.text_field :twitter_username %>
  </p>
  <p>
    <%= f.label :facebook_fan_page %><br />
    <%= f.text_field :facebook_fan_page %>
  </p>
  <p>
    <%= f.label :summary %><br />
    <%= f.text_area :summary %>
  </p>
  <p>
    <%= f.label :homepage_html %><br />
    <%= f.text_area :homepage_html %>
  </p>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', venues_path %>
